var PreCache = {
    bg: {
        width: width,
        height: height,
        img: function(c) {
            c.background(255);
            c.strokeWeight(22);
            for (var i = 0; i < height; i += 10) {
                c.stroke(lerpColor(color(200, 0, 0), color(255, 136, 0), i / height));
                c.line(0, i, width, i);
            }

            for (var Size = 79; Size < 600; Size += 10) {
                c.fill(0, 0, 0, 10);
                c.noStroke();
                c.rect(0, 799, width, -Size);
            }


            return c;
        },
    },
